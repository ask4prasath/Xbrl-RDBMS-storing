<h1>Listing iacieis</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iacieis.each do |iaciei| %>
  <tr>
    <td><%= iaciei.company_id %></td>
    <td><%= iaciei.value %></td>
    <td><%= link_to 'Show', iaciei %></td>
    <td><%= link_to 'Edit', edit_iaciei_path(iaciei) %></td>
    <td><%= link_to 'Destroy', iaciei, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

