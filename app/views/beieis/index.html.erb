<h1>Listing beieis</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @beieis.each do |beiei| %>
  <tr>
    <td><%= beiei.company_id %></td>
    <td><%= beiei.value %></td>
    <td><%= link_to 'Show', beiei %></td>
    <td><%= link_to 'Edit', edit_beiei_path(beiei) %></td>
    <td><%= link_to 'Destroy', beiei, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

